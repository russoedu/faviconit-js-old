{
  "source": "./src",
  "destination": "./docs",
  "includes": ["\\.js$"],
  "plugins": [
    {
      "name": "esdoc-node-plugin",
      "option": {
        "base": ".",
        "devDependencies": false
      }
    },
    {
      "name": "esdoc-standard-plugin",
      "option": {
        "brand": {
          "title": "Faviconit",
          "description": "Faviconit :: free favicon & apple touch icon creator to all devices and browsers.",
          "repository": "https://github.com/russoedu/faviconit",
          "site": "http://faviconit.com"
        }
      }
    },
    {
      "name": "esdoc-node"
    }
  ]
}
